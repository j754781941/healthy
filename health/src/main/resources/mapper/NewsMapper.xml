<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wdk.healthy.dao.NewsMapper">

    <select id="getNews" resultType="com.wdk.healthy.pojo.NewsInfo">
        select * from news_info where created_at;
    </select>
    <select id="queryStringNews" resultType="java.lang.String">
        SELECT content from news_info where id = 2
    </select>
</mapper>
